<h1>Coding Events</h1>

@if (ViewBag.events.Count == 0)
            {
                <p>No events yet.</p>
            }

<table>
    <tr>
        <th>Event Name</th>
        <th>Event Description</th>
    </tr>
    @foreach (var meeting in ViewBag.events)
    {
        <tr>
            <td>@meeting.Key</td>
            <td>@meeting.Value</td>
        </tr>
        
    }
</table>