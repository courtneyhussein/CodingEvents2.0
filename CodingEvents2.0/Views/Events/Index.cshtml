<h1>Coding Events</h1>

@if (ViewBag.events.Count == 0)
            {
                <p>No events yet.</p>
            }

<table>
    <tr>
        <th>ID</th>
        <th>Event Name</th>
        <th>Event Description</th>
    </tr>
    @foreach (Event meeting in ViewBag.events)
    {
<tr>
    <td>@meeting.Id</td>
    <td>@meeting.Name</td>
    <td>@meeting.Description</td>
</tr>
        
    }
</table>